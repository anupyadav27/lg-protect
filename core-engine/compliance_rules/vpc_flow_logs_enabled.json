{
  "Compliance Name": "cis_4.0_aws",
  "ID": "2.9",
  "Name": "VPC Flow Logs Enabled",
  "Description": "Ensure VPC flow logging is enabled in all VPCs",
  "Function Name": "vpc_flow_logs_enabled",
  "clients": ["ec2"],
  "functions": {
    "ec2_vpcs": {
      "method": "describe_vpcs",
      "field_path": "Vpcs[].VpcId"
    },
    "ec2_flow_logs": {
      "method": "describe_flow_logs",
      "field_path": "FlowLogs[].ResourceId"
    }
  },
  "evaluation": {
    "type": "subset",
    "source_client": "ec2_vpcs",
    "target_client": "ec2_flow_logs",
    "field": "VpcId"
  }
}
