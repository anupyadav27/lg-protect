{
  "Compliance Name": "iso27001_2022_aws",
  "ID": "A.8.16",
  "Name": "Logging Enabled",
  "Description": "Ensure logging is enabled for CloudFront distributions.",
  "Function Name": "cloudfront_distributions_logging_enabled",
  "clients": ["cloudfront"],
  "functions": {
    "cloudfront": {
      "method": "get_distribution_config",
      "field_path": "DistributionConfig.Logging.Enabled"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "cloudfront",
    "field": "Enabled"
  }
}
