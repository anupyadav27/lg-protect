{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "Neptune.5",
  "Name": "Neptune DB clusters should not use public subnets",
  "Description": "This control checks whether Neptune DB clusters are configured to use public subnets.",
  "Function Name": "neptune_cluster_uses_public_subnet",
  "clients": ["neptune"],
  "functions": {
    "neptune": {
      "method": "describe_db_clusters",
      "field_path": "DBClusters[].DBSubnetGroup"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "neptune",
    "field": "DBSubnetGroup"
  }
}
