{
  "Compliance Name": "aws_well_architected_framework_security_pillar_aws",
  "ID": "SEC02-BP03",
  "Name": "Secrets Management",
  "Description": "Check for secrets embedded in AWS Lambda function code.",
  "Function Name": "awslambda_function_no_secrets_in_code",
  "clients": ["lambda"],
  "functions": {
    "lambda": {
      "method": "get_function",
      "field_path": "Code.Location"
    }
  },
  "evaluation": {
    "type": "not exists",
    "source_client": "lambda",
    "field": "Location"
  }
}
