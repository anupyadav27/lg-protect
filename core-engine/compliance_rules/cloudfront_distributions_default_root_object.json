{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "CloudFront.1",
  "Name": "Default Root Object",
  "Description": "Ensure CloudFront distributions have a default root object configured.",
  "Function Name": "cloudfront_distributions_default_root_object",
  "clients": ["cloudfront"],
  "functions": {
    "cloudfront": {
      "method": "get_distribution_config",
      "field_path": "DistributionConfig.DefaultRootObject"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "cloudfront",
    "field": "DefaultRootObject"
  }
}
