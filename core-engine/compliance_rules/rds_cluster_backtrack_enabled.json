{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "RDS.12",
  "Name": "IAM authentication should be configured for RDS clusters",
  "Description": "This control checks whether an RDS DB cluster has IAM database authentication enabled.",
  "Function Name": "rds_cluster_backtrack_enabled",
  "clients": ["rds"],
  "functions": {
    "rds": {
      "method": "describe_db_clusters",
      "field_path": "DBClusters[].BacktrackWindow"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "rds",
    "field": "BacktrackWindow",
    "value": 0
  }
}
