{
  "Compliance Name": "cis_4.0_aws",
  "ID": "1.14",
  "Name": "IAM Access Keys Rotated Every 90 Days",
  "Description": "Ensure access keys are rotated every 90 days or less",
  "Function Name": "iam_rotate_access_key_90_days",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "get_access_key_last_used",
      "field_path": "AccessKeyLastUsed.LastUsedDate"
    }
  },
  "evaluation": {
    "type": "age_less_than",
    "source_client": "iam",
    "field": "LastUsedDate",
    "max_days": 90
  }
}
