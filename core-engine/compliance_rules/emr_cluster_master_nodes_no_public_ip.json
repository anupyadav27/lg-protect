{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "EMR.1",
  "Name": "Amazon EMR cluster master nodes should not have public IP addresses",
  "Description": "This control checks whether Amazon EMR clusters have master nodes with public IP addresses.",
  "Function Name": "emr_cluster_master_nodes_no_public_ip",
  "clients": ["emr"],
  "functions": {
    "emr": {
      "method": "describe_cluster",
      "field_path": "Cluster.Ec2InstanceAttributes.Ec2SubnetId"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "emr",
    "field": "Ec2SubnetId"
  }
}
