{
  "Compliance Name": "cis_4.0_aws",
  "ID": "1.8",
  "Name": "IAM Password Policy Minimum Length 14",
  "Description": "Ensure IAM password policy requires minimum length of 14 or greater",
  "Function Name": "iam_password_policy_minimum_length_14",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "get_account_password_policy",
      "field_path": "PasswordPolicy.MinimumPasswordLength"
    }
  },
  "evaluation": {
    "type": "greater_than_or_equal",
    "source_client": "iam",
    "field": "MinimumPasswordLength",
    "min_value": 14
  }
}
