{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "DynamoDB.4",
  "Name": "DynamoDB tables should be present in a backup plan",
  "Description": "This control checks whether Amazon DynamoDB tables are present in AWS Backup plans.",
  "Function Name": "dynamodb_accelerator_cluster_multi_az",
  "clients": ["dax"],
  "functions": {
    "dax": {
      "method": "describe_clusters",
      "field_path": "Clusters[].ClusterArn"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "dax",
    "field": "ClusterArn"
  }
}
