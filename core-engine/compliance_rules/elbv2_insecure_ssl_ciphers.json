{
  "Compliance Name": "fedramp_moderate_revision_4_aws",
  "ID": "sc-8",
  "Name": "Transmission Integrity (SC-8)",
  "Description": "The information system protects the confidentiality AND integrity of transmitted information.",
  "Function Name": "elbv2_insecure_ssl_ciphers",
  "clients": ["elbv2"],
  "functions": {
    "elbv2": {
      "method": "describe_ssl_policies",
      "field_path": "SslPolicies[].Ciphers[].Name"
    }
  },
  "evaluation": {
    "type": "find",
    "source_client": "elbv2",
    "target_client": "elbv2",
    "field": "CipherName",
    "insecure_ciphers": [
      "DES-CBC3-SHA",
      "RC4-MD5",
      "NULL-SHA",
      "NULL-MD5",
      "EXP-DES-CBC-SHA",
      "EXP-RC2-CBC-MD5",
      "EXP-RC4-MD5"
    ]
  }
}
