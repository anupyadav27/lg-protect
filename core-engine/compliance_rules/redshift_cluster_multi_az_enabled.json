{
  "Compliance Name": "fedramp_low_revision_4_aws",
  "ID": "ca-7",
  "Name": "Continuous Monitoring (CA-7)",
  "Description": "Continuously monitor configuration management processes. Determine security impact, environment and operational risks.",
  "Function Name": "redshift_cluster_multi_az_enabled",
  "clients": ["redshift"],
  "functions": {
    "redshift": {
      "method": "describe_clusters",
      "field_path": "Clusters[].MultiAZ"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "redshift",
    "field": "MultiAZ",
    "value": true
  }
}
