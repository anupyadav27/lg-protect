{
  "Compliance Name": "aws_well_architected_framework_security_pillar_aws",
  "ID": "SEC02-BP03",
  "Name": "Secrets Management",
  "Description": "Check for secrets stored in AWS Lambda environment variables.",
  "Function Name": "awslambda_function_no_secrets_in_variables",
  "clients": ["lambda"],
  "functions": {
    "lambda": {
      "method": "get_function_configuration",
      "field_path": "Environment.Variables"
    }
  },
  "evaluation": {
    "type": "not exists",
    "source_client": "lambda",
    "field": "Variables"
  }
}
