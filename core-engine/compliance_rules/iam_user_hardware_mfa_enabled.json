{
  "Compliance Name": "fedramp_low_revision_4_aws",
  "ID": "ac-2",
  "Name": "Account Management (AC-2)",
  "Description": "Manage system accounts, group memberships, privileges, workflow, notifications, deactivations, and authorizations.",
  "Function Name": "iam_user_hardware_mfa_enabled",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "list_users",
      "field_path": "Users[].UserName"
    },
    "iam_mfa": {
      "method": "list_mfa_devices",
      "field_path": "MFADevices[].UserName"
    }
  },
  "evaluation": {
    "type": "subset",
    "source_client": "iam",
    "target_client": "iam_mfa",
    "field": "UserName"
  }
}
