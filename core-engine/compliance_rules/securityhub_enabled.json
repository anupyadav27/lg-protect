{
  "Compliance Name": "cis_4.0_aws",
  "ID": "4.16",
  "Name": "AWS Security Hub Enabled",
  "Description": "Ensure AWS Security Hub is enabled",
  "Function Name": "securityhub_enabled",
  "clients": ["securityhub"],
  "functions": {
    "securityhub": {
      "method": "describe_hub",
      "field_path": "HubArn"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "securityhub",
    "field": "HubArn"
  }
}
