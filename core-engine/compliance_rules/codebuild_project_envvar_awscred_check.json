{
  "Compliance Name": "iso27001_2022_aws",
  "ID": "A.8.4",
  "Name": "Access To Source Code",
  "Description": "Read and write access to source code, development tools and software libraries should be appropriately managed.",
  "Function Name": "codebuild_project_envvar_awscred_check",
  "clients": ["codebuild"],
  "functions": {
    "codebuild": {
      "method": "list_projects",
      "field_path": "projects[]"
    },
    "codebuild": {
      "method": "batch_get_projects",
      "field_path": "projects[].environment.variables[].name"
    }
  },
  "evaluation": {
    "type": "not exists",
    "source_client": "codebuild",
    "field": "name",
    "value": "AWS_ACCESS_KEY_ID"
  }
}
