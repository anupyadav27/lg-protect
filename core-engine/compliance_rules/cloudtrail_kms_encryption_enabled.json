{
  "Compliance Name": "cis_4.0_aws",
  "ID": "3.5",
  "Name": "",
  "Description": "Ensure CloudTrail logs are encrypted at rest using KMS CMKs",
  "Function Name": "cloudtrail_kms_encryption_enabled",
  "clients": ["cloudtrail"],
  "functions": {
    "cloudtrail": {
      "method": "describe_trails",
      "field_path": "trailList[].KmsKeyId"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "cloudtrail",
    "field": "KmsKeyId"
  }
}
