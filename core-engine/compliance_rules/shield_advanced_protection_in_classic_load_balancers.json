{
  "Compliance Name": "ens_rd2022_aws",
  "ID": "mp.s.4.r1.aws.shieldadv.1",
  "Name": "Shield Advanced Protection for Classic Load Balancers",
  "Description": "Detección y reacción",
  "Function Name": "shield_advanced_protection_in_classic_load_balancers",
  "clients": ["shield", "elb"],
  "functions": {
    "shield": {
      "method": "list_protections",
      "field_path": "Protections[].ResourceArn"
    },
    "elb": {
      "method": "describe_load_balancers",
      "field_path": "LoadBalancerDescriptions[].LoadBalancerName"
    }
  },
  "evaluation": {
    "type": "subset",
    "source_client": "elb",
    "target_client": "shield",
    "field": "LoadBalancerName"
  }
}
