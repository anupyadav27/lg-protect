{
  "Compliance Name": "fedramp_low_revision_4_aws",
  "ID": "au-2",
  "Name": "API Gateway Logging Enabled",
  "Description": "Ensure API Gateway logging is enabled to audit events and enhance security investigations.",
  "Function Name": "apigateway_restapi_logging_enabled",
  "clients": ["apigateway"],
  "functions": {
    "apigateway": {
      "method": "get_rest_apis",
      "field_path": "items[].id"
    },
    "apigateway": {
      "method": "get_stage",
      "field_path": "item[].loggingLevel"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "apigateway",
    "field": "loggingLevel"
  }
}
