{
  "Compliance Name": "iso27001_2022_aws",
  "ID": "A.8.14",
  "Name": "Redundancy of information processing facilities",
  "Description": "Information processing facilities should be implemented with redundancy sufficient to meet availability",
  "Function Name": "networkfirewall_multi_az",
  "clients": ["networkfirewall", "ec2"],
  "functions": {
    "networkfirewall": {
      "method": "describe_firewall",
      "field_path": "Firewall.SubnetMappings[].AvailabilityZone"
    },
    "ec2": {
      "method": "describe_availability_zones",
      "field_path": "AvailabilityZones[].ZoneName"
    }
  },
  "evaluation": {
    "type": "subset",
    "source_client": "networkfirewall",
    "target_client": "ec2",
    "field": "AvailabilityZone"
  }
}
