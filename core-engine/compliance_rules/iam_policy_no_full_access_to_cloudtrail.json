{
  "Compliance Name": "ens_rd2022_aws",
  "ID": "op.exp.8.r4.aws.ct.1",
  "Name": "IAM Policy No Full Access to CloudTrail",
  "Description": "Control de acceso",
  "Function Name": "iam_policy_no_full_access_to_cloudtrail",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "get_policy_version",
      "field_path": "PolicyVersion.Document.Statement[].Action"
    }
  },
  "evaluation": {
    "type": "not_in",
    "source_client": "iam",
    "field": "Action",
    "forbidden_values": ["cloudtrail:*", "*"]
  }
}
