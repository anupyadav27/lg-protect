{
  "Compliance Name": "iso27001_2022_aws",
  "ID": "A.8.16",
  "Name": "Web ACL Rule Logging",
  "Description": "Ensure WAFv2 Web ACL rules have logging enabled for monitoring anomalous behavior.",
  "Function Name": "wafv2_webacl_rule_logging_enabled",
  "clients": ["wafv2"],
  "functions": {
    "wafv2": {
      "method": "get_logging_configuration",
      "field_path": "LoggingConfiguration.ResourceArn"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "wafv2",
    "field": "ResourceArn"
  }
}
