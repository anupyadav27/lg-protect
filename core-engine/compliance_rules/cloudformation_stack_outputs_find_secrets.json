{
  "Compliance Name": "aws_well_architected_framework_security_pillar_aws",
  "ID": "SEC02-BP03",
  "Name": "Secrets Management",
  "Description": "Check for secrets in CloudFormation stack outputs.",
  "Function Name": "cloudformation_stack_outputs_find_secrets",
  "clients": ["cloudformation"],
  "functions": {
    "cloudformation": {
      "method": "describe_stacks",
      "field_path": "Stacks[].Outputs[].OutputValue"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "cloudformation",
    "field": "OutputValue"
  }
}
