{
  "Compliance Name": "ens_rd2022_aws",
  "ID": "mp.com.3.aws.cf.1",
  "Name": "HTTPS Enabled",
  "Description": "Ensure HTTPS is enabled for CloudFront distributions.",
  "Function Name": "cloudfront_distributions_https_enabled",
  "clients": ["cloudfront"],
  "functions": {
    "cloudfront": {
      "method": "get_distribution_config",
      "field_path": "DistributionConfig.ViewerCertificate.CloudFrontDefaultCertificate"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "cloudfront",
    "field": "CloudFrontDefaultCertificate"
  }
}
