{
  "Compliance Name": "aws_well_architected_framework_security_pillar_aws",
  "ID": "SEC02-BP03",
  "Name": "Secrets Management",
  "Description": "Check for secrets in EC2 launch configurations.",
  "Function Name": "autoscaling_find_secrets_ec2_launch_configuration",
  "clients": ["autoscaling"],
  "functions": {
    "autoscaling": {
      "method": "describe_auto_scaling_groups",
      "field_path": "AutoScalingGroups[].LaunchConfigurationName"
    },
    "autoscaling": {
      "method": "describe_launch_configurations",
      "field_path": "LaunchConfigurations[].UserData"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "autoscaling",
    "field": "UserData"
  }
}
