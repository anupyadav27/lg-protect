{
  "Compliance Name": "ens_rd2022_aws",
  "ID": "mp.com.1.aws.elb.2",
  "Name": "ELB Uses Secure SSL Ciphers",
  "Description": "Per√≠metro seguro",
  "Function Name": "elb_insecure_ssl_ciphers",
  "clients": ["elb"],
  "functions": {
    "elb": {
      "method": "describe_load_balancer_policies",
      "field_path": "PolicyDescriptions[].PolicyAttributeDescriptions[].AttributeValue"
    }
  },
  "evaluation": {
    "type": "not_in",
    "source_client": "elb",
    "field": "AttributeValue",
    "forbidden_values": ["Protocol-SSLv2", "Protocol-SSLv3", "Protocol-TLSv1"]
  }
}
