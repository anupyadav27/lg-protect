{
  "Compliance Name": "cis_4.0_aws",
  "ID": "1.8",
  "Name": "IAM Password Policy Number",
  "Description": "Ensure IAM password policy requires at least one number",
  "Function Name": "iam_password_policy_number",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "get_account_password_policy",
      "field_path": "PasswordPolicy.RequireNumbers"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "iam",
    "field": "RequireNumbers",
    "value": true
  }
}
