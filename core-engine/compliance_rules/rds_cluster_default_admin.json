{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "RDS.3",
  "Name": "RDS DB instances should have encryption at rest enabled",
  "Description": "This control checks whether storage encryption is enabled for your RDS DB instances.",
  "Function Name": "rds_cluster_default_admin",
  "clients": ["rds"],
  "functions": {
    "rds": {
      "method": "describe_db_clusters",
      "field_path": "DBClusters[].MasterUsername"
    }
  },
  "evaluation": {
    "type": "not_exists",
    "source_client": "rds",
    "field": "MasterUsername",
    "value": "admin"
  }
}
