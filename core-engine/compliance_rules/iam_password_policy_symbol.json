{
  "Compliance Name": "cis_4.0_aws",
  "ID": "1.9",
  "Name": "Ensure IAM password policy requires at least one symbol",
  "Description": "Password policies are used to enforce password complexity requirements.",
  "Function Name": "iam_password_policy_symbol",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "get_account_password_policy",
      "field_path": "PasswordPolicy.RequireSymbols"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "iam",
    "field": "RequireSymbols",
    "value": true
  }
}
