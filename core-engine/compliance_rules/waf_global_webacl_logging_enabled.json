{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "WAF.1",
  "Name": "AWS WAF Classic Global Web ACL logging should be enabled",
  "Description": "This control checks whether logging is enabled on AWS WAF global web ACLs.",
  "Function Name": "waf_global_webacl_logging_enabled",
  "clients": ["waf"],
  "functions": {
    "waf": {
      "method": "get_logging_configuration",
      "field_path": "LoggingConfiguration.LogDestinationConfigs[]"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "waf",
    "field": "LogDestinationConfigs"
  }
}
