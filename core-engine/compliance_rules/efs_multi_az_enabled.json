{
  "Compliance Name": "iso27001_2022_aws",
  "ID": "A.8.14",
  "Name": "EFS Multi-AZ Enabled",
  "Description": "Information processing facilities should be implemented with redundancy sufficient to meet availability",
  "Function Name": "efs_multi_az_enabled",
  "clients": ["efs"],
  "functions": {
    "efs": {
      "method": "describe_file_systems",
      "field_path": "FileSystems[].AvailabilityZoneName"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "efs",
    "field": "AvailabilityZoneName"
  }
}
