{
  "Compliance Name": "pci_3.2.1_aws",
  "ID": "10.1",
  "Name": "Web ACL Logging Enabled",
  "Description": "Ensure WAFv2 Web ACLs have logging enabled to link access to system components to individual users.",
  "Function Name": "wafv2_web_acl_logging_enabled",
  "clients": ["wafv2"],
  "functions": {
    "wafv2": {
      "method": "get_logging_configuration",
      "field_path": "LoggingConfiguration.ResourceArn"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "wafv2",
    "field": "ResourceArn"
  }
}
