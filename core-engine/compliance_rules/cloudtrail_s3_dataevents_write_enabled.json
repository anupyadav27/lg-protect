{
  "Compliance Name": "cis_4.0_aws",
  "ID": "3.8",
  "Name": "",
  "Description": "Ensure that object-level logging for write events is enabled for S3 buckets",
  "Function Name": "cloudtrail_s3_dataevents_write_enabled",
  "clients": ["cloudtrail"],
  "functions": {
    "cloudtrail": {
      "method": "get_event_selectors",
      "field_path": "EventSelectors[].DataResources[].Type"
    }
  },
  "evaluation": {
    "type": "equal",
    "source_client": "cloudtrail",
    "field": "Type",
    "value": "AWS::S3::Object"
  }
}
