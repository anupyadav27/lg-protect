{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "NetworkFirewall.3",
  "Name": "Network Firewall firewalls should have deletion protection enabled",
  "Description": "This control checks whether an AWS Network Firewall firewall has deletion protection enabled.",
  "Function Name": "networkfirewall_deletion_protection",
  "clients": ["network-firewall"],
  "functions": {
    "network-firewall": {
      "method": "describe_firewall",
      "field_path": "Firewall.DeleteProtection"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "network-firewall",
    "field": "DeleteProtection",
    "value": true
  }
}
