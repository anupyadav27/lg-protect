{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "SNS.2",
  "Name": "SNS topics should be encrypted at rest using AWS KMS",
  "Description": "This control checks whether Amazon SNS topics are encrypted at rest using AWS KMS.",
  "Function Name": "sns_subscription_not_using_http_endpoints",
  "clients": ["sns"],
  "functions": {
    "sns": {
      "method": "list_subscriptions",
      "field_path": "Subscriptions[].Protocol"
    }
  },
  "evaluation": {
    "type": "not_exists",
    "source_client": "sns",
    "field": "Protocol",
    "value": "http"
  }
}
