{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "Organizations.5",
  "Name": "AWS account should be part of AWS Organizations",
  "Description": "This control checks whether the AWS account is part of AWS Organizations.",
  "Function Name": "organizations_account_part_of_organizations",
  "clients": ["organizations"],
  "functions": {
    "organizations": {
      "method": "describe_organization",
      "field_path": "Organization.Id"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "organizations",
    "field": "Id"
  }
}
