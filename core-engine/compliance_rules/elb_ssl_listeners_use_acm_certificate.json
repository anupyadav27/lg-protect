{
  "Compliance Name": "pci_4.0_aws",
  "ID": "A1.1.3.15",
  "Name": "ELB SSL Listeners Use ACM Certificate",
  "Description": "Checks if the Classic Load Balancers use SSL certificates provided by AWS Certificate Manager",
  "Function Name": "elb_ssl_listeners_use_acm_certificate",
  "clients": ["elb"],
  "functions": {
    "elb": {
      "method": "describe_load_balancers",
      "field_path": "LoadBalancerDescriptions[].ListenerDescriptions[].Listener.SSLCertificateId"
    }
  },
  "evaluation": {
    "type": "contains",
    "source_client": "elb",
    "field": "SSLCertificateId",
    "expected_value": "arn:aws:acm:"
  }
}
