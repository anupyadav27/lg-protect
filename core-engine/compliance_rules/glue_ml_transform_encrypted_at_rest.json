{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "Glue.1",
  "Name": "Glue development endpoints should be deleted if no longer needed",
  "Description": "This control checks whether AWS Glue development endpoints are present.",
  "Function Name": "glue_ml_transform_encrypted_at_rest",
  "clients": ["glue"],
  "functions": {
    "glue": {
      "method": "get_ml_transform",
      "field_path": "TransformEncryption.MlUserDataEncryption.MlUserDataEncryptionMode"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "glue",
    "field": "MlUserDataEncryptionMode"
  }
}
