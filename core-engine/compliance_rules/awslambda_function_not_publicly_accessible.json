{
  "Compliance Name": "fedramp_low_revision_4_aws",
  "ID": "sc-7",
  "Name": "Boundary Protection (SC-7)",
  "Description": "Ensure AWS Lambda functions are not publicly accessible.",
  "Function Name": "awslambda_function_not_publicly_accessible",
  "clients": ["lambda"],
  "functions": {
    "lambda": {
      "method": "get_function_configuration",
      "field_path": "VpcConfig.VpcId"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "lambda",
    "field": "VpcId"
  }
}
