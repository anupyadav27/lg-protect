{
  "Compliance Name": "cis_4.0_aws",
  "ID": "1.9",
  "Name": "IAM Password Policy Prevents Reuse",
  "Description": "Ensure IAM password policy prevents password reuse",
  "Function Name": "iam_password_policy_reuse_24",
  "clients": ["iam"],
  "functions": {
    "iam": {
      "method": "get_account_password_policy",
      "field_path": "PasswordPolicy.PasswordReusePrevention"
    }
  },
  "evaluation": {
    "type": "greater_than",
    "source_client": "iam",
    "field": "PasswordReusePrevention",
    "min_value": 24
  }
}
