{
  "Compliance Name": "pci_4.0_aws",
  "ID": "6.3.3.3",
  "Name": "ECS Service Fargate Latest Platform Version",
  "Description": "Checks if ECS Fargate services is set to the latest platform version",
  "Function Name": "ecs_service_fargate_latest_platform_version",
  "clients": ["ecs"],
  "functions": {
    "ecs": {
      "method": "describe_services",
      "field_path": "services[].platformVersion"
    }
  },
  "evaluation": {
    "type": "equals",
    "source_client": "ecs",
    "field": "platformVersion",
    "expected_value": "LATEST"
  }
}
