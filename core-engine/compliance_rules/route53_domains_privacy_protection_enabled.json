{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "Route53.3",
  "Name": "Route 53 domains should have privacy protection enabled",
  "Description": "This control checks whether Amazon Route 53 domains have privacy protection enabled.",
  "Function Name": "route53_domains_privacy_protection_enabled",
  "clients": ["route53domains"],
  "functions": {
    "route53domains": {
      "method": "get_domain_detail",
      "field_path": "AdminPrivacy"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "route53domains",
    "field": "AdminPrivacy",
    "value": true
  }
}
