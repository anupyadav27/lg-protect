{
  "Compliance Name": "cis_4.0_aws",
  "ID": "5.6",
  "Name": "VPC Peering Routing Tables with Least Privilege",
  "Description": "Ensure routing tables for VPC peering are least access",
  "Function Name": "vpc_peering_routing_tables_with_least_privilege",
  "clients": ["ec2"],
  "functions": {
    "ec2": {
      "method": "describe_vpc_peering_connections",
      "field_path": "VpcPeeringConnections[].VpcPeeringConnectionId"
    },
    "ec2_routes": {
      "method": "describe_route_tables",
      "field_path": "RouteTables[].Routes[].VpcPeeringConnectionId"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "ec2",
    "field": "VpcPeeringConnectionId"
  }
}
