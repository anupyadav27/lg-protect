{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "Inspector.1",
  "Name": "Amazon Inspector ECR container scanning should be enabled",
  "Description": "This control checks whether Amazon Inspector scans ECR container images that are pushed to repositories.",
  "Function Name": "inspector2_active_findings_exist",
  "clients": ["inspector2"],
  "functions": {
    "inspector2": {
      "method": "list_findings",
      "field_path": "findings[].status"
    }
  },
  "evaluation": {
    "type": "not_exists",
    "source_client": "inspector2",
    "field": "status",
    "value": "ACTIVE"
  }
}
