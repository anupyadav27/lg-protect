{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "WorkSpaces.1",
  "Name": "WorkSpaces should be encrypted",
  "Description": "This control checks whether Amazon WorkSpaces are encrypted.",
  "Function Name": "workspaces_volume_encryption_enabled",
  "clients": ["workspaces"],
  "functions": {
    "workspaces": {
      "method": "describe_workspaces",
      "field_path": "Workspaces[].VolumeEncryptionKey"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "workspaces",
    "field": "VolumeEncryptionKey"
  }
}
