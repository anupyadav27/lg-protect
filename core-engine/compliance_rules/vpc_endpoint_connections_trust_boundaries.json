{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "VPC.7",
  "Name": "VPC flow logging should be enabled",
  "Description": "This control checks whether Amazon VPC Flow Logs are found and enabled for VPCs.",
  "Function Name": "vpc_endpoint_connections_trust_boundaries",
  "clients": ["ec2"],
  "functions": {
    "ec2": {
      "method": "describe_vpc_endpoints",
      "field_path": "VpcEndpoints[].PolicyDocument"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "ec2",
    "field": "PolicyDocument"
  }
}
