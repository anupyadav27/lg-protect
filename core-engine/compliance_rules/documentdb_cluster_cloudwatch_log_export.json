{
  "Compliance Name": "aws_foundational_security_best_practices_aws",
  "ID": "DocumentDB.3",
  "Name": "DocumentDB clusters should have audit logging enabled",
  "Description": "This control checks whether a DocumentDB cluster has audit logging enabled.",
  "Function Name": "documentdb_cluster_cloudwatch_log_export",
  "clients": ["docdb"],
  "functions": {
    "docdb": {
      "method": "describe_db_clusters",
      "field_path": "DBClusters[].EnabledCloudwatchLogsExports[]"
    }
  },
  "evaluation": {
    "type": "exists",
    "source_client": "docdb",
    "field": "EnabledCloudwatchLogsExports"
  }
}
